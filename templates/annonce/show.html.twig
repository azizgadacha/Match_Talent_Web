{% extends 'base.html.twig' %}

{% block title %}Annonce{% endblock %}

{% block body %}
    <h1>Annonce</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdAnnonce</th>
                <td>{{ annonce.idAnnonce }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ annonce.titre }}</td>
            </tr>
            <tr>
                <th>NomSocieté</th>
                <td>{{ annonce.nomSocieté }}</td>
            </tr>
            <tr>
                <th>Datedebut</th>
                <td>{{ annonce.datedebut ? annonce.datedebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Datefin</th>
                <td>{{ annonce.datefin ? annonce.datefin|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ annonce.description }}</td>
            </tr>
            <tr>
                <th>TypeContrat</th>
                <td>{{ annonce.typeContrat }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_annonce_index') }}">back to list</a>

    <a href="{{ path('app_annonce_edit', {'idAnnonce': annonce.idAnnonce}) }}">edit</a>

    {{ include('annonce/_delete_form.html.twig') }}
{% endblock %}
